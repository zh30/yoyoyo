{
  "name": "yoyoyo",
  "version": "0.12.0",
  "private": true,
  "repository": "https://github.com/zhanghecool/yoyoyo.git",
  "author": "hertz <zhanghecool@outlook.com>",
  "license": "MIT",
  "engines": {
    "node": ">=10.15.3",
    "npm": ">=6.4.1"
  },
  "scripts": {
    "cz": "git add . && npx git-cz",
    "webpack": "NODE_ENV=production webpack --watch",
    "dev": "pnpm webpack & pnpm serve & wait",
    "serve": "bundle exec jekyll serve",
    "binit": "bundle install",
    "build-gem": "gem build script/yoyoyo.gemspec",
    "build-js": "pnpm webpack",
    "release": "semantic-release"
  },
  "dependencies": {
    "@babel/runtime": "^7.17.9",
    "animejs": "^3.2.1",
    "core-js": "^3.21.1",
    "rxjs": "^6.6.7",
    "swup": "^2.0.14"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.9",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-numeric-separator": "^7.16.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.17.3",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@commitlint/cli": "^8.3.6",
    "@commitlint/config-conventional": "^8.3.6",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.1",
    "@semantic-release/npm": "^7.1.3",
    "@semantic-release/release-notes-generator": "^9.0.3",
    "@types/animejs": "^3.1.4",
    "autoprefixer": "^10.4.4",
    "babel-loader": "^8.2.4",
    "husky": "^4.3.8",
    "postcss": "^8.4.12",
    "semantic-release": "^17.4.7",
    "tailwindcss": "^3.0.23",
    "tslint": "^6.1.3",
    "typescript": "^3.9.10",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12",
    "workbox-webpack-plugin": "^5.1.4"
  },
  "release": {
    "branches": [
      "master",
      "develop"
    ],
    "debug": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  },
  "main": "index.js"
}
