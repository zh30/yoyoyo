{
  "name": "yoyoyo",
  "version": "0.5.5",
  "private": true,
  "repository": "https://github.com/zhanghecool/yoyoyo.git",
  "author": "hertz <zhanghecool@outlook.com>",
  "license": "MIT",
  "engines": {
    "node": ">=10.15.3",
    "npm": ">=6.4.1"
  },
  "scripts": {
    "cz": "git add . && npx git-cz",
    "webpack": "NODE_ENV=production webpack --watch",
    "start": "yarn webpack & yarn serve & wait",
    "serve": "bundle exec jekyll serve",
    "init": "bundle install",
    "build-gem": "gem build script/yoyoyo.gemspec",
    "build-js": "yarn webpack",
    "release": "semantic-release"
  },
  "dependencies": {
    "@babel/runtime": "^7.10.2",
    "animejs": "^3.2.0",
    "core-js": "^3.6.5",
    "rxjs": "^6.5.5",
    "swup": "^2.0.9"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/npm": "^7.0.5",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-proposal-numeric-separator": "^7.10.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.1",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-typescript": "^7.10.1",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/animejs": "^3.1.1",
    "babel-loader": "^8.1.0",
    "husky": "^4.2.5",
    "semantic-release": "^17.0.8",
    "tslint": "^6.1.2",
    "typescript": "^3.9.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "workbox-webpack-plugin": "^5.1.3"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "release": {
    "branches": [
      "master",
      "develop"
    ],
    "debug": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  }
}
